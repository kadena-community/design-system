{
  "kda": {
    "foundation": {
      "border": {
        "width": {
          "$type": "dimension",
          "hairline": {
            "$value": 1
          },
          "normal": {
            "$value": 2
          },
          "thick": {
            "$value": 4
          }
        },
        "hairline": {
          "$type": "border",
          "$value": {
            "width": "{kda.foundation.border.width.hairline}",
            "style": "solid",
            "color": "{kda.foundation.color.border.base.default}"
          }
        },
        "normal": {
          "$type": "border",
          "$value": {
            "width": "{kda.foundation.border.width.normal}",
            "style": "solid",
            "color": "{kda.foundation.color.border.base.@bold}"
          }
        },
        "thick": {
          "$type": "border",
          "$value": {
            "width": "{kda.foundation.border.width.thick}",
            "style": "solid",
            "color": "{kda.foundation.color.border.base.@bold}"
          }
        }
      }
    }
  }
}
