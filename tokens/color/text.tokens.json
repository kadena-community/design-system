{
  "kda": {
    "foundation": {
      "color": {
        "$type": "color",
        "text": {
          "base": {
            "default": {
              "$description": "Use for primary text, such as body copy, sentence case headers, and buttons.",
              "$value": "{kda.foundation.color.neutral.n95}"
            },
            "@disabled": {
              "$description": "Use for text in a disabled state.",
              "$value": "{kda.foundation.color.neutral.n60}"
            },
            "@selected": {
              "$description": "Use for text in selected or opened states, such as tabs and dropdown buttons.",
              "$value": "{kda.foundation.color.palette.blue.n70}"
            },
            "@inverse": {
              "$description": "Use for text on bold backgrounds.",
              "$value": "{kda.foundation.color.neutral.n0}"
            }
          },
          "gray": {
            "default": {
              "$value": "{kda.foundation.color.neutral.n70}"
            },
            "@lighter": {
              "$value": "{kda.foundation.color.neutral.n60}"
            },
            "@bolder": {
              "$value": "{kda.foundation.color.neutral.n80}"
            },
            "@inverse": {
              "$value": "{kda.foundation.color.neutral.n30}"
            }
          },
          "subtle": {
            "default": {
              "$value": "{kda.foundation.color.neutral.n70}"
            },
            "@hover": {
              "$value": "{kda.foundation.color.neutral.n80}"
            },
            "@inverse": {
              "$value": "{kda.foundation.color.neutral.n30}"
            }
          },
          "subtlest": {
            "default": {
              "$value": "{kda.foundation.color.neutral.n60}"
            },
            "@hover": {
              "$value": "{kda.foundation.color.neutral.n50}"
            },
            "@inverse": {
              "$value": "{kda.foundation.color.neutral.n40}"
            }
          },
          "brand": {
            "primary": {
              "default": {
                "$value": "{kda.foundation.color.brand.primary.n95}"
              },
              "@inverse": {
                "$value": "{kda.foundation.color.brand.primary.n5}"
              }
            },
            "secondary": {
              "default": {
                "$value": "{kda.foundation.color.brand.secondary.n95}"
              },
              "@inverse": {
                "$value": "{kda.foundation.color.brand.secondary.n5}"
              }
            }
          },
          "semantic": {
            "positive": {
              "default": {
                "$value": "{kda.foundation.color.semantic.positive.n90}"
              },
              "@inverse": {
                "$value": "{kda.foundation.color.semantic.positive.n10}"
              }
            },
            "negative": {
              "default": {
                "$value": "{kda.foundation.color.semantic.negative.n90}"
              },
              "@inverse": {
                "$value": "{kda.foundation.color.semantic.negative.n10}"
              }
            },
            "warning": {
              "default": {
                "$value": "{kda.foundation.color.semantic.warning.n90}"
              },
              "@inverse": {
                "$value": "{kda.foundation.color.semantic.warning.n10}"
              }
            },
            "info": {
              "default": {
                "$value": "{kda.foundation.color.semantic.info.n90}"
              },
              "@inverse": {
                "$value": "{kda.foundation.color.semantic.info.n10}"
              }
            }
          }
        }
      }
    }
  }
}
